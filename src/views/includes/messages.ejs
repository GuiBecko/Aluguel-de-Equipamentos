<% if (errors.length > 0) { %>

<div class="erro-container">
    <% errors.forEach(erro =>{ %>
        <%= erro %> <br>
    <% }) %>
</div>

<% } %>

<% if (success.length > 0) { %>

<div class="row">
    <div class="col my-3">
        <div class="alert alert-success">
            <% success.forEach(erro =>{ %>
                <%= erro %> <br>
            <% }) %>
        </div>
    </div>
</div>

<% } %>